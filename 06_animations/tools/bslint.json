{
    "extends": "bslint:recommended",
    "rules": {
        "no-print": "warn",
        "no-stop": "warn",
        "no-todo": "warn",
        "unused-variable": "warn",
        "unsafe-path-loop": "warn",
        "unsafe-iterators": "warn",
        "unreachable-code": "warn",
        "eol-last": "off",
        "no-trailing-whitespace": "warn",
        "consistent-trailing-comma": "off"
    }
}
