<?xml version="1.0" encoding="utf-8"?>
<component name="navItem" extends="baseComponent">
    <script type="text/brightscript" uri="navItem.brs"/>
    <interface>
        <field id="itemContent" type="node" onChange="onContentChanged"/>
        <field id="focusPercent" type="float" onChange="onFocusPercentChanged"/>
        <field id="rowHasFocus" type="boolean"  onChange="onFocusPercentChanged"/>
    </interface>
    <children>
        <rectangle id="rectangle" visible="false">
            <multiStyleLabel id="label"/>
        </rectangle>
    </children>
</component>